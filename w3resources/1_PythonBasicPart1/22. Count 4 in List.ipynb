   "source": [
    "Write a Python program to count the number 4 in a given list."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "def count_4(number):\n",
    "    list_of_number = list(str(number))\n",
    "    count = 0\n",
    "    for i in list_of_number:\n",
    "        if i == '4':\n",
    "            count += 1\n",
    "    return count"
   ]
  },
